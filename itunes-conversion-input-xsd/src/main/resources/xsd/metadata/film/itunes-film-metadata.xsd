    <xs:simpleType name="FileName">
        <xs:restriction base="xs:token">
            <xs:pattern value="[^/: ]+"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FormattedBoolean">
        <xs:restriction base="xs:string">
            <xs:pattern value="\s*([Tt][Rr][Uu][Ee])|([Ff][Aa][Ll][Ss][Ee]){0,1}\s*"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FormattedDate">
        <xs:restriction base="xs:date">
            <xs:pattern value="\s*\d{4}-((0[1-9])|(1[0-2]))-(0[1-9]|[12][0-9]|3[01]){0,1}\s*"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FormattedPositiveInteger">
        <xs:restriction base="xs:string">
            <xs:pattern value="[0-9]*"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FormattedTime">
        <xs:restriction base="xs:token">
            <xs:pattern value="\s*\d{1,2}:\d{2}:\d{2}((.\d{3})|([:;]\d{2}))?\s*"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FormattedYear">
        <xs:restriction base="xs:string">
            <xs:pattern value="([0-9]{4}){0,1}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Isrc">
        <xs:restriction base="xs:string">
            <xs:pattern value="[a-zA-Z0-9]{5}[0-9]{7}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Eidr">
        <xs:restriction base="importer:NonEmptyTokenString"/>
    </xs:simpleType>
    <xs:simpleType name="Iswc">
        <xs:restriction base="xs:string">
            <xs:pattern value="T[0-9]{10}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="LanguageType">
        <xs:restriction base="xs:token">
            <xs:minLength value="1"/>
            <xs:pattern value="[a-zA-Z]{2,3}(-[a-zA-Z]{4})?(-([0-9]{3}|[a-zA-Z]{2}))?"/>
        </xs:restriction>
    </xs:simpleType>
