<bindings xmlns="http://java.sun.com/xml/ns/jaxb"
      xmlns:xsi="http://www.w3.org/2000/10/XMLSchema-instance"
      xmlns:xs="http://www.w3.org/2001/XMLSchema"
      version="2.1">
    <bindings schemaLocation="../xsd/metadata/iso-639-2-codes.xsd">
        <globalBindings typesafeEnumMaxMembers="1024" />
    </bindings>

     <bindings schemaLocation="../xsd/metadata/iso-639-2-codes.xsd">
         <schemaBindings>
             <package name="com.netflix.imfutility.generated.dpp.metadata" />
         </schemaBindings>
     </bindings>

     <bindings schemaLocation="../xsd/metadata/types.xsd">
         <schemaBindings>
             <package name="com.netflix.imfutility.generated.dpp.metadata" />
         </schemaBindings>
     </bindings>

     <bindings schemaLocation="../xsd/metadata/dpp-metadata.xsd">
         <schemaBindings>
             <package name="com.netflix.imfutility.generated.dpp.metadata" />
         </schemaBindings>
     </bindings>

     <bindings schemaLocation="../xsd/audiomap/dpp-audiomap.xsd">
         <schemaBindings>
             <package name="com.netflix.imfutility.generated.dpp.audiomap" />
         </schemaBindings>
     </bindings>

</bindings>