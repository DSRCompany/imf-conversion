<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://validation.imf.netflix.com"
           xmlns="http://validation.imf.netflix.com">

    <!--
      Root element.
    -->
    <xs:element name="errors">
        <xs:complexType>
            <xs:sequence>
                <xs:element type="xs:string" name="error" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

</xs:schema>